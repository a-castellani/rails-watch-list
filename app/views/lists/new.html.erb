<div class="container">
  <div class="row d-flex justify-content-center">
    <div class="col-5 my-3">
      <%= simple_form_for(@list) do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <%= f.submit "Add list", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
